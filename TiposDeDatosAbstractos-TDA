class Empleado:
  def __init__ (self, nombre, apellido, dni):
    self.__nombre= nombre
    self.__apellido= apellido
    self.__dni= dni

 def __repr__(self):
  return self.__nombre+" "+self.__apellido+" "+str(self.__dni)

 def getNombre(self):
  return self.__nombre
#--retorna el nombre del mayor de los dos
 def mayorEmpleado(self, self.empleado2):
  nombreMayor= self.__nombre
  if self.__dni > e2.__dni:
    nombreMayor= e2.__nombre
    return nombreMayor

